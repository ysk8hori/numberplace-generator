{
  "name": "@ysk8hori/numberplace-generator",
  "version": "3.0.0-beta.1",
  "description": "Generate number-place (Sudoku) game.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.26.2",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "sudoku",
    "numberplace",
    "game"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "rm -r ./dist & tsc --project tsconfig.publish.json && tsc-alias",
    "test": "vitest --config vitest.config.ts",
    "test:run": "vitest run --config vitest.config.ts",
    "test:index": "vitest run --config vitest.config.ts src/__tests__/index.spec.ts",
    "lint": "eslint --max-warnings=0 src",
    "lint:fix": "eslint --fix src",
    "prettier": "prettier --write .",
    "type-check": "tsc --noEmit",
    "coverage": "vitest run --coverage",
    "measure:executeion:time": "ts-node -r tsconfig-paths/register src/measureExecutionTime.ts",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prepublishOnly": "pnpm run build",
    "semantic-release": "semantic-release",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "author": "",
  "dependencies": {
    "reflect-metadata": "0.2.2",
    "tsyringe": "4.10.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/node": "24.10.4",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
