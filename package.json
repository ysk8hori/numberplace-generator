{
  "name": "@ysk8hori/numberplace-generator",
  "version": "3.0.0-beta.1",
  "description": "Generate number-place (Sudoku) game.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "sudoku",
    "numberplace",
    "game"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "rm -r ./dist & tsc --project tsconfig.publish.json && tsc-alias",
    "test": "vitest --config vitest.config.ts",
    "test:run": "vitest run --config vitest.config.ts",
    "test:index": "vitest run --config vitest.config.ts src/__tests__/index.spec.ts",
    "lint:fix": "eslint --fix src",
    "prettier": "prettier --write .",
    "type-check": "tsc --noEmit",
    "coverage": "vitest run --coverage",
    "measure:executeion:time": "ts-node -r tsconfig-paths/register src/measureExecutionTime.ts",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "author": "",
  "dependencies": {
    "reflect-metadata": "^0.2.0",
    "tsyringe": "^4.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "semantic-release": "23.0.8",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "vitest": "1.5.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
